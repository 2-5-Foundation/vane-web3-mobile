{
  "name": "vane_lib",
  "version": "0.1.0",
  "type": "module",
  "main": "main.ts",
  "module": "./dist/main.mjs",
  "types": "main.ts",
  "exports": {
    ".": {
      "types": "./dist/main.d.ts",
      "import": "./dist/main.mjs",
      "require": "./dist/main.js"
    }
  },
  "scripts": {
    "clean": "rm -rf dist pkg/host_functions",
    "prebuild": "mkdir -p pkg/host_functions && cp -R ../host_functions/*.ts pkg/host_functions/ && sed -i '' -e \"s#'../vane_lib/primitives'#'../../primitives'#g\" pkg/host_functions/*.ts",
    "build": "bun run clean && bun run prebuild && bun build ./main.ts --outdir dist && bun run build:types",
    "build:types": "tsc --emitDeclarationOnly --outDir dist"
  },
  "devDependencies": {
    "@types/bun": "latest"
  },
  "peerDependencies": {
    "typescript": "^5.0.0"
  },
  "dependencies": {
    "@solana-program/system": "^0.8.1",
    "@solana/kit": "^4.0.0",
    "@solana/spl-token": "^0.4.14",
    "@solana/web3.js": "^1.98.4",
    "viem": "^2.21.50"
  },
  "files": [
    "pkg",
    "dist",
    "main.ts",
    "networking.ts",
    "primitives.ts"
  ]
}